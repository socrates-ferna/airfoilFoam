FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.00012321;

ejection    uniform 0.0004275387;

boundaryField
{
    Symmetry1
    {
        type empty;
    }

    Symmetry2
    {
        type empty;
    }

    FarField
    {
        type freestream;
        freestreamValue $internalField;
    }
    Ejection_1
    {
        type fixedValue;
        value $ejection;
    }
    Ejection_2
    {
        type fixedValue;
        value $ejection;
    }

    Foil
    {
        type            kqRWallFunction;
        value           uniform 0;
    }
}
// ************************************************************************* //
