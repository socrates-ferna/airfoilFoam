FoamFile
{
    version 2.0;
    format ascii;
    class volScalarField;
    location "0";
    object nut;
}
dimensions [ 0 2 -1 0 0 0 0 ];
internalField uniform     3.24218014034413E-06;
ejection uniform    5.40363356724022E-06;

boundaryField
{
    Symmetry1
    {
        type empty;
    }

    Symmetry2
    {
        type empty;
    }

    FarField
    {
        type freestream;
        freestreamValue $internalField;
    }
    Ejection_1
    {
        type fixedValue;
        value $ejection;
    }
    Ejection_2
    {
        type fixedValue;
        value $ejection;
    }

    Foil
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
}

