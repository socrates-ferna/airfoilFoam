FoamFile
{
    version 2.0;
    format ascii;
    class volScalarField;
    location "0";
    object nuTilda;
}
dimensions [ 0 2 -1 0 0 0 0 ];
internalField uniform 2.9013e-7;
boundaryField
{
    Symmetry1
    {
        type empty;
    }

    Symmetry2
    {
        type empty;
    }

    Farfield
    {
        type fixedValue;
        value $internalField;
    }

    Foil
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
}
